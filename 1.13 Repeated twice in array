//Repeated twice in array
#include<stdio.h>

int main(){
	printf("Name: Philip Gracian Sanova\nReg No:192324023\n");
	int n,i,j;
	printf("Enter the size of array :");
	scanf("%d",&n);  
	int arr[n];
	printf("\nEnter elements of array :");
	for(i=0;i<n;i++){
		scanf("%d",&arr[i]);
	}
	printf("\nElements that appear twice in this array are ");
    for(i=0;i<n;i++){
    	int temp=1;
    	for(j=i+1;j<n;j++){
    		if(arr[i]==arr[j]&&arr[i]!='\0'){
    			temp++;
    			arr[j]='\0';
			}
			
		}
		if(temp==2){
			printf("%d ",arr[i]);	
		}
	}
	return 0;
}

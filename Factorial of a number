//Find Factorial of a given number
#include <stdio.h>
int main() {
    printf("Name: Philip Gracian Sanova\nReg no: 192324023\n");
    int n,fact=1;
    printf("Enter the number: ");
    scanf("%d", &n);
    int temp=n;
    while(temp>=1){
    	fact=fact*temp;
    	temp--;
	}
	printf("\nThe factorial of %d is %d",n,fact);
    return 0;
}

#include <stdio.h>
#include <stdlib.h>
#include <string.h>
void swap(int *a,int *b){
	int temp;
	temp=*a;
	*a=*b;
	*b=temp;
	return;
}
void insert(int arr[],int size){
	int i,j;
	for(i=1;i<size;i++){
		for(j=0;j<i;j++){
			if(arr[i]<arr[j]){
				swap(&arr[i],&arr[j]);
			}
		}
	}
	return;
}
int main() {
	printf("Name: Philip Gracian Sanova\nReg no: 192324023\n");
    int n,i;
    printf("Enter the size of the array: ");
    scanf("%d",&n);
    int arr[n];
    printf("\nEnter the array elements: ");
    for(i=0;i<n;i++){
    	scanf("%d",&arr[i]);
	}
    insert(arr,n);
    printf("\nAfter Insertion Sort\n");
    for(i=0;i<n;i++){
    	printf("%d ",arr[i]);
	}
	return 0;
}

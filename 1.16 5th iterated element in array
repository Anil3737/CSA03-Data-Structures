//5th iterated element in array
#include <stdio.h>

int main() {
    printf("Name: Philip Gracian Sanova\nReg No:192324023\n");
    int n;
    printf("Enter array size:");
    scanf("%d",&n);
    int arr[n];
    printf("\nEnter array elements:");
    for(int i=0;i<n;i++)
    scanf("%d",&arr[i]);
    for(int i=0;i<n;i++){
    	if(i==4){
    		printf("%d",arr[i]);
    		break;
		}
	}
    return 0;
}
